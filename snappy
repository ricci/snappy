#!/usr/bin/env python3

import sys
import snappylib.place as place
import snappylib.commands as commands

# XXX: should be in a config file
place.Place("test", "/test")

if len(sys.argv) < 2:
    commands.usage.call()
else:
    sys.argv.pop(0)
    commands.byname.get(sys.argv.pop(0), commands.usage).call()
